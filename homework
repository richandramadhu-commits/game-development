#create dictionary
blue={}
while True:
    print("1. insert\n 2. display all names\n 3. display all passwords\n 4. get password\n 5. delete\n 6. exit")
    ask=int(input("choose an option. ")) 
    if 1==ask: 
        name=input("enter the name ")
        password=input("enter the password ")
        blue[name]=password
        print("name added")
    elif 2==ask:
        print(blue.keys())
    elif 3==ask:
        print(blue.values())